{{#if categories}}
    {{#each categories}}
        {{#unless allChildrenHaveOwnDocument}}
            <section class="tsd-panel-group tsd-member-group {{cssClasses}}">
                <h2>{{title}}（{{children.length}}）</h2>
                {{#each children}}
                    {{#unless hasOwnDocument}}
                        {{> member}}
                    {{/unless}}
                {{/each}}
            </section>
			<ins class="adsbygoogle" style="display:block; text-align:center;margin-top: 30px;" data-ad-layout="in-article" data-ad-format="fluid"
				data-ad-client="ca-pub-7962287588031867" data-ad-slot="2542544532"></ins>
			<script> (adsbygoogle = window.adsbygoogle || []).push({});</script>
        {{/unless}}
    {{/each}}
{{else}}
    {{#each groups}}
        {{#unless allChildrenHaveOwnDocument}}
            {{> members.group}}
        {{/unless}}
    {{/each}}
{{/if}}